{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Asset artifact",
  "type": "object",
  "required": ["id","agentId","type","title","metadata","timestamp"],
  "properties": {
    "id": { "type": "string" },
    "agentId": { "type": "string" },
    "type": { "const": "asset" },
    "title": { "type": "string" },
    "metadata": { "type": "object" },
    "timestamp": { "type": "string", "format": "date-time" }
  },
  "additionalProperties": true
}
