{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Plan artifact",
  "type": "object",
  "required": ["id","agentId","type","timestamp"],
  "properties": {
    "id": { "type": "string" },
    "agentId": { "type": "string" },
    "type": { "const": "plan" },
    "items": { "type": "array" },
    "timestamp": { "type": "string", "format": "date-time" }
  },
  "additionalProperties": true
}
