{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Release artifact",
  "type": "object",
  "required": ["id","agentId","type","tag","timestamp"],
  "properties": {
    "id": { "type": "string" },
    "agentId": { "type": "string" },
    "type": { "const": "release" },
    "tag": { "type": "string" },
    "notes": { "type": "string" },
    "timestamp": { "type": "string", "format": "date-time" }
  },
  "additionalProperties": true
}
